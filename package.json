{
  "name": "vibe-coding-joke",
  "displayName": "Vibe Coding (Joke)",
  "description": "%ext.description%",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "vibe-coding.start",
        "title": "%command.start%"
      },
      {
        "command": "vibe-coding.stop",
        "title": "%command.stop%"
      }
    ],
    "configuration": {
      "title": "Vibe Coding (Joke)",
      "properties": {
        "vibeCoding.editorShake.enabled": {
          "order": 0,
          "type": "boolean",
          "default": true,
          "description": "%config.editorShake.enabled%"
        },
        "vibeCoding.editorShake.intensity": {
          "order": 1,
          "type": "number",
          "default": 3,
          "minimum": 1,
          "maximum": 5,
          "description": "%config.editorShake.intensity%"
        },
        "vibeCoding.server.enabled": {
          "order": 2,
          "type": "boolean",
          "default": true,
          "description": "%config.server.enabled%"
        },
        "vibeCoding.gettingStarted": {
          "order": 99,
          "type": "null",
          "markdownDescription": "%config.gettingStarted%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/vscode": "^1.85.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "ws": "^8.16.0"
  }
}
